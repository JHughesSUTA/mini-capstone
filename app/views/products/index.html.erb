<h1>All Products</h1>
<div class="row">
  <% @products.each do |product| %>
    <div class="col-md-4">
      <div class="well">
        <h3><a href="/products/<%= product.id %>"><%= product.name %></a></h3>
        <img class="product-image" src="<%= product.image %>" alt="<% product.name %>"> 
        <h4><%= product.price %></h4>
        <h4><%= product.description %></h4>
      </div>
    </div> 
  <%end%>
</div>
<a href="/products/new">Add new product</a>
